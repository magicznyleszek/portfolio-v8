---
layout: none
---
<?xml version='1.0' encoding='UTF-8'?>
<urlset xmlns='http://www.sitemaps.org/schemas/sitemap/0.9'>

    <!-- home page -->
    <url>
        <loc>http://leszekpietrzak.com/</loc>
    </url>
    <url>
        <loc>http://leszekpietrzak.com/en-us</loc>
    </url>

    <!-- RSS feeds -->
    <url>
        <loc>http://leszekpietrzak.com/rss.xml</loc>
    </url>
    <url>
        <loc>http://leszekpietrzak.com/en-us/rss.xml</loc>
    </url>

    <!-- all projects -->
    {% for post in site.posts %}
        <url>
            <loc>{{ site.url }}{{ post.url }}</loc>
        </url>
    {% endfor %}

    <!-- all categories -->
    {% for category in site.data.categories %}
        <url>
            <loc>{{ site.url }}{{ category.url }}</loc>
        </url>
        <url>
            <loc>{{ site.url }}{{ category.url-en-us }}</loc>
        </url>
    {% endfor %}
</urlset>